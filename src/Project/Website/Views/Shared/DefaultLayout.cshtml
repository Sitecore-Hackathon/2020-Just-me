@using Constellation.Foundation.ModelMapping
@using Sitecore.Data
@using Sitecore.ExperienceForms.Mvc.Html
@using Sitecore.Mvc
@using Sitecore.Mvc.Analytics.Extensions
@using Sitecore.Mvc.Presentation
@*@using Sitecore.Mvc.Analytics.Extensions*@
@model Sitecore.Mvc.Presentation.RenderingModel
@{
    Layout = null;
 
}
<!DOCTYPE html>
<html lang="@Model.Item.Language.Name">
<head>
    <meta charset="utf-8">
    <meta content="IE=edge" http-equiv=X-UA-Compatible>
    <meta name="viewport" content="initial-scale=1.0, maximum-scale=1, width=device-width, user-scalable=no" />
    <title>@Sitecore.Context.Item.Name</title>
    <link rel="stylesheet" type="text/css" href="~/fee/dist/css/app.css"/>

    @Html.Sitecore().VisitorIdentification()
    @Html.Sitecore().Placeholder("HtmlHead")
    @*@Html.Sitecore().VisitorIdentification()*@
</head>
<body>
    
    <header>
        @Html.Sitecore().Placeholder("Header")
    </header>


    <main id="main" style="margin-top: 45px;">

        @Html.Sitecore().Placeholder("Content")
    </main>


    @Html.Sitecore().Placeholder("Footer")

    @Html.RenderFormStyles()
    @Html.RenderFormScripts()

<script src="~/fee/dist/js/app.js"></script>

</body>
</html>
